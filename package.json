{
  "name": "klk-iamonlyoneman",
  "version": "1.0.0",
  "description": "KlK - iamonlyoneman",
  "main": "src/server/index.ts",
  "scripts": {
    "preinstall": "node -e \"!process.env.npm_config_user_agent.startsWith('pnpm/') && !console.log('Use \\`npx pnpm install\\` to install dependencies in this repository\\n') && process.exit(1)\"",
    "server-compile": "tsc --noEmit -p ./tsconfig-server.json",
    "~server-compile": "tsc -w --noEmit -p ./tsconfig-server.json",
    "server-run": "ts-node --project ./tsconfig-server.json src/server/index.ts",
    "~server-run": "nodemon -e js,ts,json -x \"npm run --silent server-run\"",
    "server-build": "rm -fr dist/server/* && tsc -p ./tsconfig-server.json",
    "server-poll": "ts-node --project ./tsconfig-server.json src/server/fullPoll.ts",
    "client-compile": "tsc --noEmit -p ./tsconfig-client.json",
    "~client-compile": "tsc -w --noEmit -p ./tsconfig-client.json",
    "~client-run": "webpack-dev-server --config webpack.dev.js",
    "client-build": "webpack --config webpack.prod.js",
    "test": "jest",
    "~test": "jest --watchAll",
    "lint": "eslint --cache --fix --max-warnings 0 --ext .ts,.tsx src/ test/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gurimarukin/klk-iamonlyoneman.git"
  },
  "author": "Gurimarukin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gurimarukin/klk-iamonlyoneman/issues"
  },
  "homepage": "https://github.com/gurimarukin/klk-iamonlyoneman#readme",
