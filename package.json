    "@types/express-serve-static-core": "^4.17.9",
    "@types/jest": "^26.0.9",
    "@types/mongodb": "^3.5.25",
    "@types/node": "^14.0.27",
    "@types/react": "^16.9.46",
    "@types/react-dom": "^16.9.8",
    "@types/react-lazy-load-image-component": "^1.5.1",
