{
  "name": "klk-iamonlyoneman",
  "version": "1.0.0",
  "description": "KlK - iamonlyoneman",
  "main": "src/server/index.ts",
  "scripts": {
    "preinstall": "node -e \"!process.env.npm_config_user_agent.startsWith('pnpm/') && !console.log('Use \\`npx pnpm install\\` to install dependencies in this repository\\n') && process.exit(1)\"",
    "server-compile": "tsc --noEmit -p ./tsconfig-server.json",
    "~server-compile": "tsc -w --noEmit -p ./tsconfig-server.json",
    "server-run": "ts-node --project ./tsconfig-server.json src/server/index.ts",
    "~server-run": "nodemon -e js,ts,json -x \"npm run --silent server-run\"",
    "server-build": "rm -fr dist/server/* && tsc -p ./tsconfig-server.json",
    "server-poll": "ts-node --project ./tsconfig-server.json src/server/fullPoll.ts",
    "server-fill-size": "ts-node --project ./tsconfig-server.json src/server/addMissingSize.ts",
    "server-create-user": "ts-node --project ./tsconfig-server.json src/server/createUser.ts",
    "client-compile": "tsc --noEmit -p ./tsconfig-client.json",
    "~client-compile": "tsc -w --noEmit -p ./tsconfig-client.json",
    "~client-run": "webpack-dev-server --config webpack.dev.js",
    "client-build": "webpack --config webpack.prod.js",
    "test": "jest",
    "~test": "jest --watchAll",
    "lint": "eslint --cache --fix --max-warnings 0 --ext .ts,.tsx src/ test/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gurimarukin/klk-iamonlyoneman.git"
  },
  "author": "Gurimarukin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gurimarukin/klk-iamonlyoneman/issues"
  },
  "homepage": "https://github.com/gurimarukin/klk-iamonlyoneman#readme",
  "nodemonConfig": {
    "ignore": [
      "src/client/*"
    ]
  },
  "devDependencies": {
    "@emotion/core": "^10.0.35",
    "@emotion/styled": "^10.0.27",
    "@phc/argon2": "^1.0.9",
    "@type-config/strict": "^1.0.2",
    "@types/dateformat": "^3.0.1",
    "@types/express": "^4.17.8",
    "@types/express-serve-static-core": "^4.17.12",
    "@types/jest": "^26.0.13",
    "@types/mongodb": "^3.5.27",
    "@types/node": "^14.6.4",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "@types/react-lazy-load-image-component": "^1.5.1",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "axios": "^0.19.2",
    "css-loader": "^4.2.2",
    "dateformat": "^3.0.3",
    "dotenv-webpack": "^2.0.0",
    "eslint": "^7.8.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.0",
    "express": "^4.17.1",
    "file-loader": "^6.1.0",
    "fork-ts-checker-webpack-plugin": "^5.1.0",
    "fp-ts": "^2.8.2",
    "fp-ts-contrib": "^0.1.18",
    "history": "^5.0.0",
    "html-webpack-plugin": "^4.4.1",
    "hyper-ts": "^0.6.0",
    "import-sort-parser-typescript": "^6.0.0",
    "io-ts": "^2.2.10",
    "jest": "^26.4.2",
    "mongodb": "^3.6.1",
    "monocle-ts": "^2.3.3",
    "newtype-ts": "^0.3.4",
    "nodemon": "^2.0.4",
    "prettier": "^2.1.1",
    "probe-image-size": "^5.0.0",
    "qs": "^6.9.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-lazy-load-image-component": "^1.5.0",
    "style-loader": "^1.2.1",
    "ts-jest": "^26.3.0",
    "ts-loader": "^8.0.3",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7",
    "upash": "^1.0.2",
    "uuid": "^8.3.0",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.1.3"
  },
  "importSort": {
    ".js, .jsx, .ts, .tsx": {
      "style": "module",
      "parser": "typescript"
    }
  }
}
