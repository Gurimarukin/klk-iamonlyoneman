{
  "name": "klk-iamonlyoneman",
  "version": "1.0.0",
  "description": "KlK - iamonlyoneman",
  "main": "src/server/index.ts",
  "scripts": {
    "preinstall": "node -e \"!process.env.npm_config_user_agent.startsWith('pnpm/') && !console.log('Use \\`npx pnpm install\\` to install dependencies in this repository\\n') && process.exit(1)\"",
    "server-compile": "tsc --noEmit -p ./tsconfig-server.json",
    "~server-compile": "tsc -w --noEmit -p ./tsconfig-server.json",
    "server-run": "ts-node --project ./tsconfig-server.json src/server/index.ts",
    "~server-run": "nodemon -e js,ts,json -x \"npm run --silent server-run\"",
    "server-build": "rm -fr dist/server/* && tsc -p ./tsconfig-server.json",
    "server-poll": "ts-node --project ./tsconfig-server.json src/server/fullPoll.ts",
    "server-fill-size": "ts-node --project ./tsconfig-server.json src/server/addMissingSize.ts",
    "server-create-user": "ts-node --project ./tsconfig-server.json src/server/createUser.ts",
    "client-compile": "tsc --noEmit -p ./tsconfig-client.json",
    "~client-compile": "tsc -w --noEmit -p ./tsconfig-client.json",
    "~client-run": "webpack-dev-server --config webpack.dev.js",
    "client-build": "rm -fr dist/client/* && webpack --config webpack.prod.js",
    "test": "jest",
    "~test": "jest --watchAll",
    "lint": "eslint --cache --fix --max-warnings 0 --ext .ts,.tsx src/ test/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gurimarukin/klk-iamonlyoneman.git"
  },
  "author": "Gurimarukin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gurimarukin/klk-iamonlyoneman/issues"
  },
  "homepage": "https://github.com/gurimarukin/klk-iamonlyoneman#readme",
  "nodemonConfig": {
    "ignore": [
      "src/client/*"
    ]
  },
  "devDependencies": {
    "@emotion/core": "^10.1.1",
    "@emotion/styled": "^10.0.27",
    "@phc/argon2": "^1.0.9",
    "@type-config/strict": "^1.1.1",
    "@types/dateformat": "^3.0.1",
    "@types/express": "^4.17.11",
    "@types/express-serve-static-core": "^4.17.18",
    "@types/jest": "^26.0.20",
    "@types/mongodb": "^3.6.6",
    "@types/node": "^14.14.25",
    "@types/qs": "6.9.7",
    "@types/react": "^16.14.3",
    "@types/react-dom": "^16.9.10",
    "@types/react-lazy-load-image-component": "^1.5.1",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.15.0",
    "@typescript-eslint/parser": "^4.15.0",
    "axios": "^0.21.1",
    "css-loader": "^4.3.0",
    "dateformat": "^3.0.3",
    "dotenv-webpack": "^2.0.0",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-fp-ts": "^0.2.1",
    "eslint-plugin-functional": "^3.2.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "express": "^4.17.1",
    "file-loader": "^6.2.0",
    "fork-ts-checker-webpack-plugin": "^5.2.1",
    "fp-ts": "^2.9.5",
    "history": "^5.0.0",
    "html-webpack-plugin": "^4.5.1",
    "hyper-ts": "^0.6.1",
    "import-sort-parser-typescript": "^6.0.0",
    "io-ts": "^2.2.14",
    "jest": "^26.6.3",
    "mongodb": "^3.6.4",
    "monocle-ts": "^2.3.5",
    "newtype-ts": "^0.3.4",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "probe-image-size": "^5.0.0",
    "qs": "^6.9.6",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-lazy-load-image-component": "^1.5.1",
    "style-loader": "^1.3.0",
    "swr": "^0.3.11",
    "ts-jest": "^26.5.1",
    "ts-loader": "^8.0.17",
    "ts-node": "^8.10.2",
    "typescript": "^4.1.4",
    "upash": "^1.0.2",
    "uuid": "^8.3.2",
    "webpack": "^4.46.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.3"
  },
  "importSort": {
    ".js, .jsx, .ts, .tsx": {
      "style": "module",
      "parser": "typescript"
    }
  }
}
